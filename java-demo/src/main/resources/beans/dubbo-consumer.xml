<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="pirate-merchant-server" owner="tp" />
	<dubbo:registry address="zookeeper://10.166.224.4:2181" />
	<dubbo:protocol name="dubbo" port="29003" />
	<dubbo:provider retries="0" timeout="100000" />
	<dubbo:consumer check="false" />

	<dubbo:reference id="demoService" interface="com.panjin.dubbo.example.DemoService"/>
	
</beans>